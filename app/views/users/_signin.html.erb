
<div id='login_form'>
	<h1 id='ex1'>X</h1>
	<%= form_for :user, url: signinpost_path do |f| %>


	<p class='mini_title'>
	  <%= f.label :email %><br>
	 <%= image_tag 'email.png', id: 'email_icon', class: 'icon' %> <%= f.text_field :email, :class => 'input' %>
	</p>

	<p class='mini_title'>
	  <%= f.label :password %><br>
	  <%= image_tag 'password.png', id: 'password_icon', class: 'icon' %><%= f.password_field :password, :class => 'input' %>
	</p>


	<p>
	  <%= f.submit "Log In", :id => 'sign_mit' %>
	</p>

	<% end %>
</div>