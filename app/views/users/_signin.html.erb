
<div id='login_form'>
	<%= form_for :user, url: signinpost_path do |f| %>


	<p class='mini_title'>
	  <%= f.label :email %><br>
	  <%= f.text_field :email %>
	</p>

	<p class='mini_title'>
	  <%= f.label :password %><br>
	  <%= f.password_field :password %>
	</p>


	<p>
	  <%= f.submit :class => 'sign_mit' %>
	</p>

	<% end %>
</div>
